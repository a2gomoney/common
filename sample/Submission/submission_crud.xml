scwin.btn_search_onclick = function (e) {
    scwin.callRestApi(scwin.url, "get");
};

scwin.btn_save_onclick = function (e) {
    const datasetMap = {
            domain: dlt_domain,
            business: dlt_biz,
            component: dlt_component,
            function: dlt_function
        };

    let targetName = tac1.getLabelText(tac1.getSelectedTabIndex());
    let ds = datasetMap[targetName];
    const reqJSON = ds.getAllJSON().filter(item => item.rowStatus !== "R");
    const method = { C: "POST", U: "PUT", D: "DELETE" };
    //rowStatus 값을 가지고 개발하는 경우 아래 사용이 무의미함 (건 by 건) (비효율적)
    reqJSON.forEach(item => {
        const option = {
                id: "sbm_call_open_api",
                action : scwin.url, 
                method : method[item.rowStatus],
                //target : 'data:json,{"id":ds,"key":"data"}',
                isProcessMsg: true
            };
        $c.nova.trxDataAsync(option, data);  
    });
};

scwin.callRestApi = async function(url, method, category ) {
    let data ={
       // "id": scwin.id
    };

    let isAsync = rdo_syncAsync.getValue();
    if(isAsync) {

        const option = {
                id: "sbm_call_open_api",
                action : url, 
                method : method.toUpperCase(),
                //target : 'data:json,{"id":"dlt","key":"data"}', 
                isProcessMsg: false
            };

        let resData = await $c.nova.trxDataSync(option, data);    
 
        if(!$c.util.isEmpty(resData)) {
            const datasetMap = {
                domain: dlt_domain,
                business: dlt_biz,
                component: dlt_component,
                function: dlt_function
            };
  
            let targetName = tac1.getLabelText(tac1.getSelectedTabIndex());
            const ds = datasetMap[targetName];
            let resJson = resData && resData.responseJSON;
            if(!$c.util.isEmpty(resJson.content)) {
                ds.removeAll();
                ds.setJSON(resJson.content);
            } else {
                //단건
                $c.win.alert(resJson.content);
                console.log(resJson.content);
            }
        }

    } else {
        const option = {
                id: "sbm_call_rest_api",
                action : url, 
                method : method.toUpperCase(),
                //target : 'data:json,{"id":"dlt_codeGrp","key":"data"}',
                submitDoneHandler: "scwin.callbackDomain",
                submitFailHandler: "scwin.callbackDomainFail",
                submitErrorHandler: "scwin.callbackDomainError",
                isProcessMsg: false
            };        
        $c.nova.trxDataAsync(option, data);    
    }
};

scwin.btn_add_onclick = function (e) {
    let selectedTac = tac1.getSelectedTabIndex();
    let targetName = tac1.getLabelText(selectedTac);
    let tacWindow = tac1.getWindow(selectedTac);
    if(targetName === "domain") {
        dlt_domain.insertRow(0);
        dlt_domain.setCellData(0, 'id', scwin.id);
    } else if(targetName === "business") {
        dlt_biz.insertRow(0);
        dlt_biz.setCellData(0, 'id', scwin.id);
    } else if(targetName === "component") {
        dlt_component.insertRow(0);
        dlt_component.setCellData(0, 'id', scwin.id);
    }  else if(targetName === "function") {
        dlt_function.insertRow(0);
        dlt_function.setCellData(0, 'id', scwin.id);
    }
};

scwin.btn_del_onclick = function (e) {
    let selectedTac = tac1.getSelectedTabIndex();
    let targetName = tac1.getLabelText(selectedTac);
    let tacWindow = tac1.getWindow(selectedTac);
    if(targetName === "domain") {
        dlt_domain.removeRow(dlt_domain.getRealRowIndex())
    } else if(targetName === "business") {
        dlt_biz.removeRow(dlt_biz.getRealRowIndex());
    } else if(targetName === "component") {
        dlt_component.removeRow(dlt_component.getRealRowIndex());
    }  else if(targetName === "function") {
        dlt_function.removeRow(dlt_function.getRealRowIndex());
    }
};

